# 服务相关日志 pattern

# TEXT
TEXT .*?
# 分隔/TEXT
TEXT_OR_SEP ( | .+? )

# YEAR: 2019
YEAR \d\d\d\d
# MONTH: 01,02,...,12
MONTH 0[1-9]|1[0-2]
# DAY: 01,02,...,31
DAY 0[1-9]|[12][0-9]|3[01]
# DATE: 2019-02-30
DATE %{YEAR}-%{MONTH}-%{DAY}

# HOUR: 01
HOUR 0[0-9]|1[0-9]|2[0-3]
# MINUTE: 01
MINUTE [0-5][0-9]
# SECOND: 01
SECOND [0-5][0-9]
# TIME
TIME %{HOUR}:%{MINUTE}:%{SECOND}([:.,]\d+)?

# 请求计数
COUNTER \d+

# 日志级别
LOGLEVEL ([Aa]lert|ALERT|[Tt]race|TRACE|[Dd]ebug|DEBUG|[Nn]otice|NOTICE|[Ii]nfo|INFO|[Ww]arn?(?:ing)?|WARN?(?:ING)?|[Ee]rr?(?:or)?|ERR?(?:OR)?|[Cc]rit?(?:ical)?|CRIT?(?:ICAL)?|[Ff]atal|FATAL|[Ss]evere|SEVERE|EMERG(?:ENCY)?|[Ee]merg(?:ency)?)
